CREATE TABLE alunos (
    id NUMBER GENERATED BY DEFAULT AS IDENTITY PRIMARY KEY,
    nome VARCHAR2(100) NOT NULL,
    idade NUMBER CHECK (idade >= 6 AND idade <= 18),
    peso NUMBER CHECK (peso > 0),
    serie VARCHAR2(20),
    escola VARCHAR2(100)
    cidade VARCHAR2(50)
);
SELECT * FROM alunos;

